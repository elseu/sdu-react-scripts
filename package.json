  },
  "bugs": {
    "url": "https://github.com/elseu/sdu-react-scripts/issues"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx,.js,.jsx",
    "prepublishOnly": "npm run lint",
    "deploy": "semantic-release",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "sdu-react-scripts": "./bin/sdu-react-scripts.js"
  },
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "4.x",
    "@typescript-eslint/parser": "4.x",
    "eslint": "7.x",
    "eslint-config-react-app": "6.x",
    "eslint-config-standard": "16.x",
    "eslint-import-resolver-typescript": "2.x",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "11.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "eslint-plugin-simple-import-sort": "7.x",
    "eslint-plugin-standard": "4.x",
    "react": ">= 16.x",
    "typescript": "4.x"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "enquirer": "^2.3.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "react": "^16.14.0",
    "semantic-release": "^17.3.7",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
