      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "3.x",
    "@typescript-eslint/parser": "3.x",
    "eslint": "6.x",
    "eslint-config-react-app": "5.x",
    "eslint-config-standard": "14.x",
