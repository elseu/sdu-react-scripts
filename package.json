{
  "name": "@elseu/sdu-react-scripts",
  "version": "1.0.0",
  "private": "true",
  "author": {
    "name": "Daphne Smit",
    "email": "D.smit@sdu.nl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elseu/sdu-react-scripts"
  },
  "bugs": {
    "url": "https://github.com/elseu/sdu-react-scripts/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/rollup"
    ]
  },
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "build": "preconstruct build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint --fix . --ext .ts,.tsx,.js,.jsx",
    "release": "yarn workspaces run release"
  },
  "prettier": "./node_modules/@elseu/sdu-react-scripts-prettier",
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@manypkg/cli": "^0.19.1",
    "@preconstruct/cli": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
