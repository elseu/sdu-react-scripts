{
  "name": "@elseu/sdu-react-scripts-rollup",
  "description": "A standard Rollup config to get started with",
  "main": "dist/elseu-sdu-react-scripts-rollup.cjs.js",
  "module": "dist/elseu-sdu-react-scripts-rollup.esm.js",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elseu/sdu-react-scripts"
  },
  "author": {
    "name": "Daphne Smit",
    "email": "D.smit@sdu.nl"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "monorepo": {
      "debug": true,
      "analyzeCommits": {
        "format": "angular"
      },
      "generateNotes": {
        "preset": "angular"
      }
    },
    "extends": "semantic-release-monorepo",
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      },
      {
        "name": "release",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "packageName": "@elseu/sdu-react-scripts-rollup",
          "markdownReleaseNotes": true
        }
      ]
    ]
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-url": "^6.1.0",
    "rollup": "^2.62.0",
    "rollup-plugin-node-externals": "^3.1.2"
  },
  "devDependencies": {
    "semantic-release": "^18.0.1",
    "semantic-release-monorepo": "^7.0.5",
    "semantic-release-slack-bot": "^3.4.1"
  }
}
