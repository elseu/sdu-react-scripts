{
  "name": "@elseu/sdu-react-scripts-eslint",
  "description": "A standard Eslint config to get started with",
  "main": "index.js",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elseu/sdu-react-scripts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "author": {
    "name": "Daphne Smit",
    "email": "D.smit@sdu.nl"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "bin": {
    "sdu-react-scripts-eslint": "./bin/sdu-react-scripts.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "monorepo": {
      "debug": true,
      "analyzeCommits": {
        "format": "angular"
      },
      "generateNotes": {
        "preset": "angular"
      }
    },
    "extends": "semantic-release-monorepo",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "packageName": "@elseu/sdu-react-scripts-eslint",
          "markdownReleaseNotes": true
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": ">=17",
    "typescript": ">=4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^6.2.0"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "semantic-release": "^22.x",
    "semantic-release-monorepo": "^8.x",
    "semantic-release-slack-bot": "^4.0.2",
    "typescript": "^5.0.4"
  }
}
