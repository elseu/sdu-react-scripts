{
  "name": "@sdu-react-scripts/babel-plugins",
  "description": "A set of Babel plugins for your project",
  "main": "dist/sdu-react-scripts-babel-plugins.cjs.js",
  "module": "dist/sdu-react-scripts-babel-plugins.esm.js",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Daphne Smit",
    "email": "D.smit@sdu.nl"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      },
      {
        "name": "fix/monorepo",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "packageName": "@sdu-react-scripts/babel-plugins"
        }
      ]
    ]
  },
  "devDependencies": {
    "semantic-release": "^18.0.1",
    "semantic-release-monorepo": "^7.0.5"
  }
}
