{
  "name": "@elseu/sdu-react-scripts-update-deps",
  "description": "Helper package to download all @elseu/sdu-titan package information and update it in the package.json",
  "main": "dist/elseu-sdu-react-scripts-update-deps.cjs.js",
  "author": "Oskar van Velden",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "sdu-deps": "./bin/sdu-update-deps.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elseu/sdu-react-scripts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "monorepo": {
      "debug": true,
      "analyzeCommits": {
        "format": "angular"
      },
      "generateNotes": {
        "preset": "angular"
      }
    },
    "extends": "semantic-release-monorepo",
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": true
      },
      {
        "name": "release",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "packageName": "@elseu/sdu-react-scripts-update-deps",
          "markdownReleaseNotes": true
        }
      ]
    ]
  },
  "dependencies": {
    "@types/jsonfile": "^6.1.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "jsonfile": "^6.1.0"
  },
  "devDependencies": {
    "semantic-release": "^19.0.5",
    "semantic-release-monorepo": "^7.0.5",
    "semantic-release-slack-bot": "^3.5.3"
  }
}
