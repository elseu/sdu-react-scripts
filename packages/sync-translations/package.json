{
  "name": "@elseu/sdu-react-scripts-sync-translations",
  "description": "Helper package to sync translations of all locales with POEditor",
  "main": "dist/elseu-sdu-react-scripts-sync-translations.cjs.js",
  "author": "Michel Vermeer",
  "version": "1.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "sdu-react-scripts-sync-translations": "./bin/sdu-sync-translations.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elseu/sdu-react-scripts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release": {
    "monorepo": {
      "debug": true,
      "analyzeCommits": {
        "format": "angular"
      },
      "generateNotes": {
        "preset": "angular"
      }
    },
    "extends": "semantic-release-monorepo",
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": true
      },
      {
        "name": "release",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "packageName": "@elseu/sdu-react-scripts-sync-translations",
          "markdownReleaseNotes": true
        }
      ]
    ]
  },
  "dependencies": {
    "commander": "^10.0.1",
    "pofile": "^1.1.4",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@types/request": "^2.48.12",
    "semantic-release": "^19.x",
    "semantic-release-monorepo": "^7.0.5",
    "semantic-release-slack-bot": "^4.0.2"
  }
}
